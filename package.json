{
  "name": "cotra",
  "packageManager": "yarn@4.0.2",
  "workspaces": {
    "packages": [
      "apps/**",
      "packages/**"
    ]
  },
  "scripts": {
    "server": "yarn workspace @cotra/server",
    "start": "yarn concurrently \"yarn server start:dev\" \"docker-compose down && docker-compose up -d\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
