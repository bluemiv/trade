{
  "name": "cotra",
  "packageManager": "yarn@3.2.2",
  "workspaces": {
    "packages": [
      "packages/**"
    ]
  },
  "scripts": {
    "web": "yarn workspace @cotra/web",
    "server": "yarn workspace @cotra/server",
    "prettier:web": "yarn prettier --write ./packages/web/src",
    "prettier:server": "yarn prettier --write ./packages/server/src"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "devDependencies": {
    "prettier": "^2.7.1"
  }
}
